<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.finalProjectJB.interlocallyapp">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET " />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InterlocallyApp">
        <activity
            android:name="com.finalProjectJB.interlocallyapp.TestActivity"
            android:exported="false" />
        <activity
            android:name="com.finalProjectJB.interlocallyapp.ProductInfo"
            android:exported="false"
            android:parentActivityName="com.finalProjectJB.interlocallyapp.MainActivity"/>
        <activity
            android:name="com.finalProjectJB.interlocallyapp.FeedbackSearch"
            android:exported="false"
            android:parentActivityName="com.finalProjectJB.interlocallyapp.MainActivity" />
        <activity
            android:name="com.finalProjectJB.interlocallyapp.Search"
            android:exported="false"
            android:parentActivityName="com.finalProjectJB.interlocallyapp.MainActivity"/>
        <activity
            android:name="com.finalProjectJB.interlocallyapp.ForgotPassword"
            android:exported="false"/>
        <activity
            android:name="com.finalProjectJB.interlocallyapp.Login"
            android:exported="true"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.finalProjectJB.interlocallyapp.Register"
            android:exported="false"/>
        <activity
            android:name="com.finalProjectJB.interlocallyapp.MainActivity"
            android:exported="false">
        </activity>

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="${MAPS_API_KEY}" />
        <activity
            android:name="com.finalProjectJB.interlocallyapp.MapsActivity"
            android:exported="false"
            android:parentActivityName="com.finalProjectJB.interlocallyapp.MainActivity"/>
    </application>

</manifest>